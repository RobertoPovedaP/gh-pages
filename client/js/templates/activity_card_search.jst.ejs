<% if(card != null && card != 'empty'){ %>
<a title="<%- card.attributes.name %>" href="#" class="js-add-comment-card" data-board-id="<%- card.attributes.board_id %>" data-card-id="<%- card.id %>" data-card-name="<%- card.attributes.name %>"><span><%- card.attributes.name %></span></a>
<%}else if(card != 'empty'){%>
    <div class="text-center">
        <span class="alert alert-info">
            <%- i18next.t('No %s available.', { postProcess: 'sprintf', sprintf: [i18next.t('cards')] }) %>
            </span>
    </div>
<%}%>
