<li class="clearfix text-center col-xs-12">
	<span class="col-xs-10"><strong><%- i18next.t('Members') %></strong></span><a class="js-close-popup pull-right" href="#"><i class="icon-remove"></i></a>		
</li>
<li class="col-xs-12 divider"></li>
<%
	board_users.each(function(board_user) {
	var added_user = users.findWhere({
		card_id: card.id,
		user_id: board_user.get('user_id')
	});
	if (_.isUndefined(added_user)) {
%>
	   <li><a href="#" class="highlight-icon js-add-card-member" data-toggle="tooltip" title="<%-board_user.attributes.full_name %> (<%-board_user.attributes.username %>)" data-user-id="<%- board_user.get('user_id') %>" data-user-name="<%- board_user.get('username') %>" data-user-fullname="<%-board_user.get('full_name') %>" data-user-initial="<%-board_user.get('initials') %>" data-user-profile-picture-path="<%-board_user.get('profile_picture_path') %>" ><%-  board_user.get('username') %></a></li>
  <%  } else { %>
		<li><a href="#" class="highlight-icon js-remove-card-member" data-card-user-id="<%-  added_user.id %>"><i class="avatar avatar-color-194 img-rounded" title="<%-  board_user.get('full_name') %> (<%-  board_user.get('username') %>)"><%-board_user.get('initials') %><%-board_user.get('username') %></i></a> <i class="icon-ok"></i></li>
<%  }
}); %> 